{
    "name": "symfony-cmf/symfony-cmf",
    "description": "Symfony Content Management Framework",
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/symfony-cmf/contributors"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/BlNARYFOUR/menu-bundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlNARYFOUR/content-bundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BlNARYFOUR/block-bundle.git"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "symfony/framework-bundle": "^2.8 || ^3.3 || ^4.0",
        "doctrine/phpcr-bundle": "^1.3 || ^2.0",
        "doctrine/phpcr-odm": "^1.4.2 || ^2.0",
        "symfony-cmf/core-bundle": "^3.0",
        "symfony-cmf/content-bundle": "^3.0",
        "symfony-cmf/menu-bundle": "^3.0",
        "symfony-cmf/block-bundle": "^3.0",
        "symfony-cmf/routing-bundle": "^2.1"
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "^9.3",
        "jackalope/jackalope-jackrabbit": "1.*"
    },
    "suggest": {
        "jackalope/jackalope-jackrabbit": "1.* as PHPCR implementation required by PHPCR-ODM",
        "jackalope/jackalope-doctrine-dbal": "1.* as PHPCR implementation required by PHPCR-ODM",
        "symfony-cmf/seo-bundle": "2.0.* for search engine optimization",
        "symfony-cmf/tree-browser-bundle": "2.0.* to support tree navigation in admins",
        "symfony-cmf/routing-auto-bundle": "2.0.* to automatically generate routes on certain events",
        "sonata-project/doctrine-phpcr-admin-bundle": "2.0.* for backend admin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
